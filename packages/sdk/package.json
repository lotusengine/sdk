{
  "name": "@lotusengine/sdk",
  "version": "1.0.16",
  "description": "LotusEngine SDK",
  "keywords": [
    "typescript",
    "lotusengine"
  ],
  "homepage": "https://github.com/lotusengine/sdk/blob/master/packages/sdk/README.md",
  "bugs": "https://github.com/lotusengine/lotusengine/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lotusengine/sdk.git"
  },
  "license": "MIT",
  "author": "LotusEngine {https://lotusengine.com/)",
  "main": "dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "imports": {
    "#sdk/*": {
      "default": "./dist/*.js"
    }
  },
  "scripts": {
    "build": "tsc --build tsconfig.build.json --force",
    "clean": "tsc --build tsconfig.build.json --clean",
    "coverage": "jest --coverage",
    "depcheck": "dependency-check . --unused --no-dev && dependency-check . --missing --no-dev",
    "prepublishOnly": "yarn build",
    "test": "jest",
    "verify": "yarn lint && yarn build && yarn depcheck && yarn coverage",
    "watch": "jest --watch",
    "wipe": "rimraf dist"
  },
  "dependencies": {
    "@lotusengine/core": "^1.0.16",
    "@lotusengine/schemas": "^1.0.16",
    "@lotusengine/types": "^1.0.16",
    "fs-extra": "^10.0.0"
  },
  "engines": {
    "node": ">=14.6.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "1f7377e6ca3312f08914d51e97bb53c643f27c2a"
}
