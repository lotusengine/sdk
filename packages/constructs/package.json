{
  "name": "@lotusengine/constructs",
  "version": "1.0.13",
  "description": "LotusEngine SDK TypeScript constructs",
  "keywords": [
    "typescript",
    "lotusengine"
  ],
  "homepage": "https://github.com/lotusengine/sdk/blob/master/packages/constructs/README.md",
  "bugs": "https://github.com/lotusengine/lotusengine/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lotusengine/sdk.git"
  },
  "imports": {
    "#constructs/*": {
      "default": "./dist/*.js"
    }
  },
  "license": "MIT",
  "author": "LotusEngine {https://lotusengine.com/)",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [],
  "scripts": {
    "build": "tsc --build tsconfig.build.json --force",
    "clean": "tsc --build tsconfig.build.json --clean",
    "coverage": "jest --coverage",
    "depcheck": "dependency-check . --unused --no-dev && dependency-check . --missing --no-dev",
    "prepublishOnly": "yarn build",
    "test": "jest",
    "verify": "yarn lint && yarn build && yarn depcheck && yarn coverage",
    "watch": "jest --watch",
    "wipe": "rimraf dist"
  },
  "dependencies": {
    "@lotusengine/core": "^1.0.13",
    "@lotusengine/schemas": "^1.0.12",
    "@lotusengine/types": "^1.0.6",
    "nanoid": "^3.1.25",
    "object-hash": "^2.2.0"
  },
  "devDependencies": {
    "@types/object-hash": "^2.1.1"
  },
  "engines": {
    "node": ">=14.6.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "1f7377e6ca3312f08914d51e97bb53c643f27c2a"
}
